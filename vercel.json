{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/images/(.*)",
      "dest": "public/images/$1"
    },
    {
      "src": "/(.*\\.css)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*\\.js)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/server.js"
    }
  ]
}
